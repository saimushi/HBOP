<?xml version="1.0" ?>
<xml>
	<section id="Index">
		<execute>
			<assign var="res" class="Auth" method="isCertification"/>
			<if var="res" val="true">
				<flow section="Index-authed" target="web"/>
			</if>
			<else>
				<flow section="web/login"/>
			</else>
		</execute>
	</section>
	<section id="Index-authed" type="web">
		<execute>
			<view section="Index" baseview="base"/>
		</execute>
	</section>
</xml>