<?xml version="1.0" ?>
<xml>
	<Index type="web">
		<construct arg1="true" arg2="string:true"/>
		<execute>
			<assign var="res" class="Auth" method="check"/>
		    <if var="res" val="true">
		        <assign var="view" class="Core" method="loadView"/>
				<return var="view"/>
	    	</if>
	    	<else>
				<new var="login" class="Login"/>
		        <assign var="view" instans="login" method="execute"/>
				<return var="view"/>
			</else>
			<return val="false"/>
	    </execute>
		<destruct/>
	</Index>
</xml>