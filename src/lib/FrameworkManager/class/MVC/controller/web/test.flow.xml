<?xml version="1.0" ?>
<xml>
	<section id="Test">
		<execute>
			<assign var="res" class="Auth" method="isCertification"/>
			<if var="res" val="true">
				<flow section="Test-authed" target="web"/>
			</if>
			<else>
				<flow section="web/login"/>
			</else>
		</execute>
	</section>
	<section id="Test-authed" type="web">
		<execute>
			<view section="Test"/>
		</execute>
	</section>
</xml>